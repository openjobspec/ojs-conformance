{
  "test_id": "L0-OPS-007",
  "level": 0,
  "category": "operations",
  "name": "fetch-multi-queue",
  "description": "PUSH jobs to different queues, then FETCH specifying multiple queues. Verify that the queue list is respected with left-to-right priority: the first queue listed is checked first.",
  "spec_ref": "ojs-core#section-7.2",
  "tags": [
    "level-0",
    "operations",
    "fetch",
    "multi-queue",
    "priority"
  ],
  "steps": [
    {
      "id": "step-1",
      "action": "POST",
      "intent": "enqueue",
      "path": "/ojs/v1/jobs",
      "headers": {
        "Content-Type": "application/openjobspec+json",
        "Accept": "application/openjobspec+json"
      },
      "body": {
        "type": "test.noop",
        "args": [
          {
            "source": "low-priority-queue"
          }
        ],
        "options": {
          "queue": "conformance-multi-low"
        }
      },
      "assertions": {
        "status": 201,
        "body": {
          "$.job.id": {
            "$exists": true
          },
          "$.job.queue": "conformance-multi-low"
        }
      }
    },
    {
      "id": "step-2",
      "action": "POST",
      "intent": "enqueue",
      "path": "/ojs/v1/jobs",
      "headers": {
        "Content-Type": "application/openjobspec+json",
        "Accept": "application/openjobspec+json"
      },
      "body": {
        "type": "test.noop",
        "args": [
          {
            "source": "high-priority-queue"
          }
        ],
        "options": {
          "queue": "conformance-multi-high"
        }
      },
      "assertions": {
        "status": 201,
        "body": {
          "$.job.id": {
            "$exists": true
          },
          "$.job.queue": "conformance-multi-high"
        }
      }
    },
    {
      "id": "step-3",
      "action": "POST",
      "intent": "fetch",
      "path": "/ojs/v1/workers/fetch",
      "headers": {
        "Content-Type": "application/openjobspec+json",
        "Accept": "application/openjobspec+json"
      },
      "body": {
        "queues": [
          "conformance-multi-high",
          "conformance-multi-low"
        ],
        "worker_id": "test-worker-1"
      },
      "assertions": {
        "status": 200,
        "body": {
          "$.jobs[0].id": "{{steps.step-2.response.body.job.id}}",
          "$.jobs[0].queue": "conformance-multi-high",
          "$.jobs[0].args[0].source": "high-priority-queue",
          "$.jobs[0].state": "active"
        }
      }
    },
    {
      "id": "step-4",
      "action": "POST",
      "intent": "fetch",
      "path": "/ojs/v1/workers/fetch",
      "headers": {
        "Content-Type": "application/openjobspec+json",
        "Accept": "application/openjobspec+json"
      },
      "body": {
        "queues": [
          "conformance-multi-high",
          "conformance-multi-low"
        ],
        "worker_id": "test-worker-1"
      },
      "assertions": {
        "status": 200,
        "body": {
          "$.jobs[0].id": "{{steps.step-1.response.body.job.id}}",
          "$.jobs[0].queue": "conformance-multi-low",
          "$.jobs[0].args[0].source": "low-priority-queue",
          "$.jobs[0].state": "active"
        }
      }
    }
  ]
}
