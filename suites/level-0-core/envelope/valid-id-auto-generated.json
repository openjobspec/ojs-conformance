{
  "test_id": "L0-ENV-009",
  "level": 0,
  "category": "envelope",
  "name": "valid-id-auto-generated",
  "description": "Enqueue a job without providing an id field. The server MUST auto-generate a valid UUIDv7 identifier for the job. The generated id MUST match the UUIDv7 format: 8-4-4-4-12 lowercase hex with version nibble '7' in the third group.",
  "spec_ref": "ojs-core#section-5.1, L0-ENV-002",
  "tags": [
    "level-0",
    "envelope",
    "id-generation",
    "uuidv7",
    "happy-path"
  ],
  "steps": [
    {
      "id": "step-1",
      "action": "POST",
      "intent": "enqueue",
      "path": "/ojs/v1/jobs",
      "headers": {
        "Content-Type": "application/openjobspec+json"
      },
      "body": {
        "type": "email.send",
        "args": [
          "user@example.com",
          "welcome"
        ]
      },
      "assertions": {
        "status": 201,
        "body": {
          "$.job.id": "string:uuidv7",
          "$.job.type": "email.send",
          "$.job.state": "available"
        }
      }
    },
    {
      "id": "step-2-second-job-different-id",
      "action": "POST",
      "intent": "enqueue",
      "path": "/ojs/v1/jobs",
      "headers": {
        "Content-Type": "application/openjobspec+json"
      },
      "body": {
        "type": "email.send",
        "args": [
          "other@example.com",
          "reset_password"
        ]
      },
      "assertions": {
        "status": 201,
        "body": {
          "$.job.id": "string:uuidv7",
          "$.job.type": "email.send",
          "$.job.state": "available"
        }
      }
    }
  ]
}
